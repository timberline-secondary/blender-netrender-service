# /etc/systemd/system/netrender-slave.service

[Unit]
Description=Monitors for users logging in locally
Documentation=https://github.com/timberline-secondary/blender-netrender-service
Wants=network-online.target
After=network-online.target

[Service]
Type=simple
ExecStart=/bin/sh /usr/local/bin/netrender/netrender-user-monitor.sh
Restart=always
RestartSec=5


[Install]
WantedBy=multi-user.target
